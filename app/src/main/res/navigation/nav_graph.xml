<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/recordFragment"
    >

    <fragment
        android:id="@+id/recordFragment"
        android:name="com.example.piano_practice_time_ranking.RecordFragment"
        android:label="fragment_record"
        tools:layout="@layout/fragment_record"
        >
        <action
            android:id="@+id/action_recordFragment_to_practiceTimeFragment"
            app:destination="@id/practiceTimeFragment"
            />
        <action
            android:id="@+id/action_recordFragment_to_rankingFragment"
            app:destination="@id/rankingFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/practiceTimeFragment"
        android:name="com.example.piano_practice_time_ranking.PracticeTimeFragment"
        android:label="fragment_practice_time"
        tools:layout="@layout/fragment_practice_time"
        >
        <action
            android:id="@+id/action_practiceTimeFragment_to_recordFragment"
            app:destination="@id/recordFragment"
            />
        <action
            android:id="@+id/action_practiceTimeFragment_to_rankingFragment"
            app:destination="@id/rankingFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/rankingFragment"
        android:name="com.example.piano_practice_time_ranking.RankingFragment"
        android:label="fragment_ranking"
        tools:layout="@layout/fragment_ranking"
        >
        <action
            android:id="@+id/action_rankingFragment_to_recordFragment"
            app:destination="@id/recordFragment"
            />
        <action
            android:id="@+id/action_rankingFragment_to_practiceTimeFragment"
            app:destination="@id/practiceTimeFragment"
            />
    </fragment>

</navigation>